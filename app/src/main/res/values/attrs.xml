<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StackImageView">
        <attr name="iconItemSize" format="dimension" />
        <attr name="borderSize" format="dimension" />
        <attr name="iconsCount" format="integer" />
        <attr name="iconOffset" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FlatButton">
        <attr name="android:text" format="string" />
        <attr name="android:background" format="reference|color" />
        <attr name="android:textColor" format="reference|color" />
    </declare-styleable>

</resources>